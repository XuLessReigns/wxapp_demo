.copy{
    position:absolute;
    right:37rpx;
    top:50%;
    margin-top:-10px;
    width:140rpx;
    border-radius:3px;
    border:1px solid #fff;
    text-align:center;
    padding-top:2px;
    padding-bottom:2px;
}
.close{
    position:fixed;
    height:90px;
    width:100%;
    bottom:0;
    left: 0;
    background:linear-gradient(to bottom, rgba(255,255,255,0.9), #ffffff);
    box-shadow:0px 1px 20px 20px rgba(255,255,255,0.8);
}
.border-close{
    left: 50%;
    bottom: 40px;
    margin-left: -15px;
    border: 2px solid #999;
    border-radius: 20px;
    width: 36px;
    height: 36px;
}

.close .border-close{
    transition: all 0.3s;
}

.close .bottom-close{
    bottom: -80px;
    transform: scale(0);
}

.border-close:after,.border-close:before{
    background-color: #999;
    width: 20px;
    margin-left: -10px;
}
.flight-ques{
    z-index: 1;
    top:40rpx;

}
.flight-ques::after{
    border:1px solid #fff;
}


.contactus_close{
    position:absolute;
    right:0;
    bottom:0px;
    border-radius:100%;
    width:120rpx;
    height:120rpx;
    box-shadow:0px 5rpx 20rpx 10rpx rgba(48, 170, 254,0.1);
    background:#fff;
}

.contactus_close:after,.contactus_close:before{
    content:"";
    position:absolute;
    width:26px;
    height:2px;
    top:50%;
    left:50%;
    margin-left:-13px;
    margin-top:-1px;
    background-color:#1ea2fe;
    transform-origin:50% 50%;
}

.contactus_close::after {
    transform:rotate(45deg);
}

.contactus_close:before{
    transform: rotate(-45deg)
}

