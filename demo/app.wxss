.mod-contact-us{
    position:fixed;
    bottom:40px;
    right:20px;
    z-index:100;
}

.mod-contact-us,.icon-phone,.icon-server,.icon-wechat{
    height: 120rpx;
    width: 120rpx;
}

.icon-phone,.icon-server,.icon-wechat{
    position: absolute;
    padding: 10rpx;
    border-radius: 100%;
    box-shadow:0px 5rpx 20rpx 10rpx rgba(48, 170, 254,0.1);
    background: #fff;
    top: 0;
    left: 0;
    transition: all .3s;
    will-change: auto;
    margin-top: 0;
}

.icon-phone image ,.icon-server image,.icon-wechat image{
    height: 100rpx;
    width: 100rpx;
}

.icon-wechat:after{
    border: none!important;
}

.contactus-open .icon-phone{
    left: -82rpx;
    top: -150rpx;
}

.contactus-open .icon-wechat{
    left: -168rpx;
}

.contactus-open .icon-server image{
    opacity: .5
}

.address-info,.address-info1{
    position: relative;
