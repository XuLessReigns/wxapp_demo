<view class="ui-toptip" hidden="{{ui_toptip.hidden !== false}}">{{ ui_toptip.msg }}</view>
<view class="ui-mask light" hidden="{{ui_toast.hidden !== false}}">
    <view class="ui-toast">{{ui_toast.msg}}</view>
</view>
<view class="ui-pageloading" hidden="{{ui_pageloading.hidden !== false}}">
    <image src="data:image/gif;base64,"></image>
</view>
<view class="ui-logo tc {{ui_logo.className}}">
    <image src="/images/logo-{{ui_logo.type || 'dark'}}.png"></image>
</view>
<image wx:if="{{!!ui_ilog_pv}}" src="{{ui_ilog_pv}}"  binderror="ilog_error"></image>
<image wx:if="{{!!ui_ilog_asm}}" src="{{ui_ilog_asm}}" binderror="ilog_error"></image>