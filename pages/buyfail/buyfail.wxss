.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding-top:150rpx;
    padding-bottom: 80rpx;
    background-color: #fff;
}

.tipfirst{
  margin-top:60rpx;
  margin-bottom: 15px;
  font-weight: bold;
}
.tipsecond{
  font-size: 12px; 
}

button{
  margin-top:150rpx;
  padding: 0px 65px;
  border-radius: 5px;
  line-height: 38px;
}

button[type="primary"] {
  color:#30aafe;
  background-color:#fff;
}

button[type="primary"]::after{
    border-color: #30aafe;
}

.mt5{
    margin-top: 5rpx;
}
.navigator-hover{
    background-color: transparent;
}